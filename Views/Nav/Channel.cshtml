@model IEnumerable<string>


@foreach (var link in Model) {


    @Html.RouteLink(link, new {
        controller = "Product",
        action = "List",
        channel = link,
        country = ViewBag.SelectedCountry,
        timeperiod = ViewBag.SelectedTimePeriod
    },
    new {
        @class = link == ViewBag.SelectedChannel ? "selected" : null
    })
}